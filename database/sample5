Schemes:
  new(A,B,C,D,E)
  thing(W,X,Y,Z)
Facts:
  new('a','b','c','d','e').
  new('a','b','b','c','b').
  new('a','a','c','e','d').
  new('a','b','c','c','e').
  new('a','a','a','a','e').
  thing('w','x','x','w').
  thing('x','y','z','z').
  thing('z','y','z','y').
  thing('w','x','x','w').
Rules:
  new(A,B,C,D,E):- thing(A,B,D,C).
Queries:
  new('a','b','c','d','e')?
  new(A,'b','b',D,E)?
  new(A,B,B,D,E)?
  new('a','b',C,D,E)?
  new(A,A,A,A,E)?
  thing(W,X,Y,Z)?
  thing(W,X,X,W)?
